	@GetMapping("/teste")
	
	public String teste() {
		return "Teste";
	}
	

@GetMapping("/jogos")
	public ArrayList<Jogo> recuperarTodos() {
		ArrayList<Jogo> lista;
		
		return lista;
	}

}
